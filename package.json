{
  "name": "@sensinum/table-field",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Table Custom Field for Strapi",
  "keywords": [
    "strapi",
    "table",
    "field",
    "custom field",
    "plugin"
  ],
  "strapi": {
    "kind": "plugin",
    "name": "table-field",
    "displayName": "Table Field",
    "description": "Strapi Custom Field with Table data structures handling."
  },
  "license": "MIT",
  "author": "VirtusLab <strapi@virtuslab.com>",
  "maintainers": [
    {
      "name": "VirtusLab // Mateusz Ziarko",
      "email": "mziarko@virtuslab.com",
      "url": "https://virtuslab.com"
    }
  ],
  "homepage": "https://github.com/VirtusLab-Open-Source/strapi-plugin-table-field#readme",
  "bugs": {
    "url": "https://github.com/VirtusLab-Open-Source/strapi-plugin-table-field/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirtusLab-Open-Source/strapi-plugin-table-field.git"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn format && yarn test:unit"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "build": "yarn clean && strapi-plugin build --minify",
    "clean": "rm -rf dist",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "test:server": "jest --forceExit --detectOpenHandles --config server/jest.config.js",
    "test:server:watch": "jest --forceExit --detectOpenHandles --config server/jest.config.js --watch",
    "test:server:ci": "CI=true jest --ci --runInBand --verbose --coverage --config server/jest.config.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.74.11",
    "react-intl": "^7.1.6",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.12.0",
    "@strapi/typescript-utils": "^5.12.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "husky": "7.0.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-styled-components": "^7.1.1",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "6.1.8",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.0.0",
    "@strapi/strapi": "^5.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=8.0.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@1.22.19"
}
